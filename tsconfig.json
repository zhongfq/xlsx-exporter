{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "noEmitHelpers": true,
    "noImplicitOverride": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
  },
  "include": [
    "./",
  ],
  "exclude": []
}